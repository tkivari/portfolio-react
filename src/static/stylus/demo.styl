.demo-container {
    border-bottom 2px solid #ccc
    margin 0px auto
    width 320px
    text-align left
    cursor pointer
    color #333
    padding 7px 9px
    background #e8e8e8

    h4 {
        margin 2px 0
    }

    .demo-content {
        font-size 13px 
        padding 10px 0
        text-align justify
    }
}

.demo-container:last-child {
    border-bottom none
}